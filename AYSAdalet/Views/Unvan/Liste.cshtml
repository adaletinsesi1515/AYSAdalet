@using AYSAdalet.Models.Modeller
@model List<Unvanlar>
@{
    ViewBag.Title = "Liste";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="card">
    <div class="header">
        <h2><strong>Ünvan</strong> Listesi - <span style="font-size:10px">Toplam Kayıt Sayısı : @ViewBag.kayitToplam</span></h2>
    </div>
    <div class="body">
        <div class="table-responsive">
            <table class="table m-b-0 table-primary" id="birimlistesi">
                <thead class="thead-light">

                    <tr>
                        <th scope="col">Ünvan ID</th>
                        <th scope="col">Ünvan</th>
                        @*<th scope="col">Etkileşim</th>*@
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.UnvanID</td>
                            <td>@item.Unvani</td>

                            @*<td>
                                    <a title="Detay" href="#" class="btn btn-success btn-sm"><i class="zmdi zmdi-account-box"></i></a>
                                    <a title="Düzenle" href="#" class="btn btn-warning btn-sm"><i class="zmdi zmdi-edit"></i></a>
                                    <a title="Sil" href="#" class="btn btn-danger btn-sm"><i class="zmdi zmdi-delete"></i></a>
                                </td>*@
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>