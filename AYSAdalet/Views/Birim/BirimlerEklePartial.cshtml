@model AYSAdalet.Models.Modeller.Birimler

@using (Html.BeginForm("BirimlerEkle", "Birim", FormMethod.Post, new { @class = "form-control" }))

{
    @Html.AntiForgeryToken()

    <div class="card">

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="body">
            
            <label>Birim Adı</label>
            @Html.TextBoxFor(model => model.BirimAdi, htmlAttributes: new { @class = "form-control" })

            
        </div>

        <div class="form-group">
            <div class="col-md-12 ">
                <button type="submit" class="btn btn-raised btn-primary btn-round waves-effect">Kayıt Ekle</button>
            </div>
        </div>
    </div>
}

