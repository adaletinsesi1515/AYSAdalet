@model IEnumerable<AYSAdalet.Models.Modeller.Bilgi_Bilgisayarlar>


@{
    ViewBag.Title = "BilgisayarEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<div class="col-lg-12 col-md-12 col-sm-12">

    <div class="card">
        <div class="header">
            <h2><strong>Bilgisayar Kasası</strong> Ekle</h2>
        </div>
        <div class="body">
            <h1>@TempData["Uyari"] </h1>

            @Html.Action("BilgisayarEklePartial", "Bilgi_Donanim")

        </div>
    </div>
</div>


<div class="card">
    <div class="header">
        <h2><strong>Bilgisayar Kasa</strong> Listesi</h2>
    </div>
    <div class="body">
        <div class="table-responsive">
            <table class="table m-b-0 table-primary" id="birimlistesi">
                <thead class="thead-light">
                    <tr>
                        <th scope="col">ID</th>
                        <th scope="col">Kasa Marka</th>
                        <th scope="col">Kasa Model</th>
                        <th scope="col">Kasa Seri No</th>
                        <th scope="col">İşlem Türü</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>

                            <td>@item.BilgisayarID</td>
                            <td>@item.BilgisayarMarka</td>
                            <td>@item.BilgisayarModel</td>
                            <td>@item.BilgisayarSeriNo</td>
                            <td>
                                <a title="Düzenle" href="/Bilgi_Donanim/BilgisayarBilgiGetir/@item.BilgisayarID" class="btn btn-warning btn-sm"><i class="zmdi zmdi-edit"></i></a>
                                <a title="Pasif Et" href="/Bilgi_Donanim/PasifEt/@item.BilgisayarID" class="btn btn-danger btn-sm"><i class="zmdi zmdi-delete"></i></a>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>



