@model AYSAdalet.Models.Modeller.Bilgi_Bilgisayarlar

@using (Html.BeginForm("BilgisayarEkle", "Bilgi_Donanim", FormMethod.Post))

{
    @Html.AntiForgeryToken()

    <div class="card">

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="body">
            <label>Bilgisayar Kasa Markası</label>
            @Html.TextBoxFor(model => model.BilgisayarMarka, htmlAttributes: new { @class = "form-control" })

            <label>Bilgisayar Kasa Modeli</label>
            @Html.TextBoxFor(model => model.BilgisayarModel, new { @class = "form-control" })

            <label>Bilgisayar Kasa Seri No</label>
            @Html.TextBoxFor(model => model.BilgisayarSeriNo, new { @class = "form-control" })
           

        </div>

        <div class="form-group">
            <div class="col-md-12 ">
                <button type="submit" class="btn btn-raised btn-primary btn-round waves-effect">Kayıt Ekle</button>
            </div>
        </div>
    </div>
}